{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "reactComponents/*": [
        "./reactComponents/*"
      ],
      "utilitiesStyles/*": [
        "./utilitiesStyles/*"
      ],
      "imgs/*": [
        "./imgs/*"
      ],
      "resources/*": [
        "./resources/*"
      ],
      "views/*": [
        "./views/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}